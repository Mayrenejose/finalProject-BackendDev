<div>
    <div class="boxTitle">
        <h1>Productos</h1>
        <div class="positionBtn">
            <button class="btnChat" id="goChat">Ir al Chat</button>
        </div>
    </div>

    <div class="bodyPage">
        {{#each dataProducts.products}}
        <div class="boxProduct">
            <img src={{this.thumbnails.[0]}} alt="products" class="image">

            <div class="boxInfo">
                <h2>{{this.title}}</h2>
                <p>{{this.description}}</p>
                <p>Precio: ${{this.price}}</p>
            </div>
        </div>
    {{/each}}
    </div>
</div>
<script src="/js/home/index.js"></script>